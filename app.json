{
  "expo": {
    "name": "Lara",
    "slug": "Lara",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./src/assets/images/icon.png",
    "scheme": "larafront",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "icon": "./src/assets/images/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.deangelis84.larafront",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.deangelis84.larafront"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false,
        "root": "./src/app"
      },
      "eas": {
        "projectId": "c85a0783-8bbd-4d3a-97ab-e31864567b76"
      }
    }
  }
}
